        <class>com.boha.golfkids.data.Country</class>
        <class>com.boha.golfkids.data.ErrorStore</class>
        <class>com.boha.golfkids.data.ErrorStoreAndroid</class>
        <class>com.boha.golfkids.data.GcmDevice</class>
        <class>com.boha.golfkids.data.GolfGroup</class>
